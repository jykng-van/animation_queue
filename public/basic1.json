{
    "name":"Basic Example 1",
    "start":[
        {
            "selector":"#sub-text, #shapes, #boxes div",
            "style":"opacity:0"
        },
        {
            "selector":"#shapes, #face",
            "style":"left:100%"
        },
        {
            "selector":"#face",
            "style":"transform:rotate(-720deg)"
        },
        {
            "selector":"#more-text",
            "style":"clip-path:inset(0% 100% 0% 0%)"
        }
    ],
    "animations":[
        {
            "steps":[
                {
                    "duration":".5s",
                    "elements":[
                        {
                            "selector":"#sub-text",
                            "style":"opacity:1"
                        }
                    ]
                },
                {
                    "duration":"1s",
                    "elements":[
                        {
                            "selector":"#shapes",
                            "style":"opacity:1; left:0"
                        }
                    ]
                },
                {
                    "duration":".5s",
                    "timing":"ease-in",
                    "elements":[
                        {
                            "selector":"#face",
                            "style":"left:0; rotate:0"
                        },
                        {
                            "selector":"#more-text",
                            "style":"clip-path:inset(0% 0% 0% 0%)"
                        }
                    ]
                },
                {
                    "duration":"1s",
                    "elements":[
                        {
                            "selector":"#boxes div",
                            "substeps":6,
                            "style":"opacity:1"
                        }
                    ]
                }

            ]
        }
    ]
}